{
  "name": "parcel-swagger-ui",
  "swagger-ui-parcel": "dist/index.html",
  "targets": {
    "swagger-ui-parcel": {
      "context": "browser",
      "outputFormat": "global",
      "scopeHoist": false,
      "isLibrary": false,
      "optimize": true,
      "includeNodeModules": true,
      "sourceMap": true,
      "source": "src/index.html",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "postinstall": "sed -i 's/#swagger-ui/$swagger-ui/' ./node_modules/swagger-ui-react/index.cjs"
  },
  "type": "module",
  "devDependencies": {
    "events": "^3.3.0",
    "parcel": "^2.9.3",
    "stream-browserify": "^3.0.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swagger-ui-react": "=5.4.2"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "alias": {
    "./node_modules/swagger-ui-react/index.mjs": "./node_modules/swagger-ui-react/index.cjs",
    "$swagger-ui": "./node_modules/swagger-ui-react/swagger-ui.js"
  }
}
